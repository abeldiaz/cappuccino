<ul>
    {{#each entry in model}}
    <li>
        <figure>
            {{img entry.name}}
            <figcaption>{{entry.name}}</figcaption>
        </figure>
        <ul>
        {{#each gestion in entry.gestions}}
            <li><a href="">{{gestion}}</a></li>
        {{/each}}
        </ul>
    </li>
    {{/each}}
</ul>
<div class="clear"></div>

